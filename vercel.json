{
  "version": 2,
  "functions": {
    "index.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.php"
    }
  ],
  "env": {
    "DB_HOST": "@db_host",
    "DB_NAME": "@db_name", 
    "DB_USER": "@db_user",
    "DB_PASS": "@db_pass"
  }
}
